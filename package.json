{
  "name": "query-pouch-gtfs",
  "version": "2.0.2",
  "description": "Tools to query data from GTFS schedules using PouchDB databases.",
  "repository": "NotWoods/query-pouch-gtfs",
  "main": "dist/index.js",
  "module": "es/index.js",
  "devDependencies": {
    "@types/geojson": "^1.0.2",
    "@types/moment-range": "^3.0.0",
    "@types/pouchdb": "^5.4.28",
    "ts-jest": "^20.0.4",
    "typescript": "^2.3.2"
  },
  "dependencies": {
    "docuri": "^4.2.2",
    "moment": "^2.18.1",
    "moment-range": "^3.0.3"
  },
  "scripts": {
    "test": "jest",
    "build-es": "tsc --module es2015 --outDir es",
    "build-cjs": "tsc --module commonjs --outDir dist",
    "prepare": "npm run build-es && npm run build-cjs"
  },
  "files": [
    "dist",
    "es",
    "index.d.ts"
  ],
  "keywords": [
    "gtfs",
    "pouchdb",
    "transit",
    "sync",
    "database",
    "transportation"
  ],
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
